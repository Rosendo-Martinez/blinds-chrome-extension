{
  "manifest_version": 3,
  "name": "Blinds",
  "version": "1.0",
  "description": "Makes it easier to focus on the important parts of a website by using blinds to cover the distracting parts!",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["scripting", "activeTab"],
  "icons": {
    "16": "images/blinds.png",
    "48": "images/blinds.png",
    "128": "images/blinds.png"
  },
  "action": {
    "default_icon": {
      "16": "images/blinds.png"
    }
  }
}